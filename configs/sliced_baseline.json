{
  "device": "mps",
  "mlflow_path": "./../eye/mlruns",
  "experiment_name": "trial_experiment",
  "data_dir": "./event_data",
  "run_name": "trial_run",
  "architecture": "CNN_GRU",
  "lr": 0.001,
  "num_epochs": 200,
  "batch_size": 24,
  "spatial_factor": 0.125,
  "temporal_subsample_factor": 0.2,
  "val_interval": 2,
  "save_k_best": 2,
  "pixel_tolerances": [
    3,
    5,
    10,
    15
  ],
  "sensor_width": 640,
  "sensor_height": 480,
  "train_stride": 15,
  "val_stride": 30,
  "train_length": 30,
  "val_length": 30,
  "n_time_bins": 3,
  "in_channels": 6,
  "voxel_grid_ch_normaization": false,
  "gamma": 0.99,
  "loss": "weighted_mse",
  "data_augmentation": {
    "prob_hflip": 0.5,
    "prob_noise": 0.3,
    "max_noise_factor": 1.2,
    "min_noise_factor": 0.05,
    "time_reversal": 0,
    "random_time_shift": 0
  }
}
