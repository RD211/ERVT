{
  "device": "cuda:4",
  "mlflow_path": "./../eye/mlruns",
  "experiment_name": "trial_experiment",
  "data_dir": "./event_data",
  "run_name": "trial_run",
  "architecture": "RVT",
  "expansion_ratio": 2,
  "mlp_act_layer": "gelu",
  "mlp_gated": true,
  "gamma": 0.99,
  "mlp_bias": true,
  "drop_prob": 0.3,
  "tbptt": 15,
  "stages": [
    {
      "kernel_size": 7,
      "output_channels": 32,
      "stride": 2,
      "partition_size": 2,
      "dim_head": 32
    },
    {
      "kernel_size": 3,
      "output_channels": 64,
      "stride": 2,
      "partition_size": 5,
      "dim_head": 32
    }
  ],
  "lr": 0.001,
  "num_epochs": 300,
  "batch_size": 8,
  "num_folds": 4,
  "random_state": 42,
  "seed": 42,	
  "spatial_factor": 0.125,
  "temporal_subsample_factor": 0.2,
  "val_interval": 4,
  "save_k_best": 2,
  "pixel_tolerances": [
    3,
    5,
    10,
    15
  ],
  "sensor_width": 640,
  "sensor_height": 480,
  "train_stride": 30,
  "val_stride": 45,
  "train_length": 45,
  "val_length": 45,
  "n_time_bins": 3,
  "voxel_grid_ch_normaization": false,
  "loss": "weighted_rmse",
  "data_augmentation": {
    "prob_hflip": 0.5,
    "prob_noise": 0.3,
    "max_noise_factor": 1.2,
    "min_noise_factor": 0.05,
    "time_reversal": 0,
    "random_time_shift": 0
  }
}
