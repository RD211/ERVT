{
    "device": "cuda:6",
    "mlflow_path": "/space/mlica/mlruns",
    "experiment_name": "trial_experiment",
    "data_dir": "/space/mlica/event_data",
    "run_name": "trial_run",
    "architecture": "RVT",
    "expansion_ratio": 2,
    "mlp_act_layer": "gelu",
    "mlp_gated": false,
    "gamma": 0.99,
    "mlp_bias": true,
    "drop_prob": 0.5,
    "tbptt": 15,
    "stages": [
      {
        "kernel_size": 13,
        "output_channels": 8,
        "stride": 2,
        "partition_size": 2,
        "dim_head": 8
      },
      {
        "kernel_size": 3,
        "output_channels": 16,
        "stride": 2,
        "partition_size": 5,
        "dim_head": 8
      }
    ],
    "lr": 0.001,
    "num_epochs": 200,
    "batch_size": 1,
    "num_folds": 5,
    "random_state": 42,
    "seed": 42,	
    "spatial_factor": 0.125,
    "temporal_subsample_factor": 0.2,
    "val_interval": 5,
    "save_k_best": 5,
    "pixel_tolerances": [
      3,
      5,
      10,
      15
    ],
    "sensor_width": 640,
    "sensor_height": 480,
    "train_stride": 15,
    "val_stride": 30,
    "train_length": 30,
    "val_length": 30,
    "in_channels": 3,
    "n_time_bins": 3,
    "voxel_grid_ch_normaization": true,
    "test_stride": 30,
    "test_length": 30,
    "loss": "weighted_rmse",
    "data_augmentation": {
      "prob_hflip": 0.5,
      "prob_noise": 0.5,
      "max_noise_factor": 2.2,
      "min_noise_factor": 0.05,
      "time_reversal": 0.5,
      "random_time_shift": 0
    }
  }
